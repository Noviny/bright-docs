{
  "name": "@brisk-docs/website",
  "version": "0.9.0",
  "description": "The Brisk website documenter",
  "main": "dist/website.cjs.js",
  "module": "dist/website.esm.js",
  "maintainers": [
    "Dean Pisani",
    "Aparna Raj",
    "Peter Yu",
    "Ben Conolly"
