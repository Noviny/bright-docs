{
    "name": "brisk-docs",
    "version": "1.0.0",
    "description": "The brisk-docs website generator",
    "main": "index.js",
    "license": "MIT",
    "private": true,
    "scripts": {
        "test": "bolt w @brisk-docs/website run jest",
        "cypress": "node_modules/.bin/cypress run",
        "dev:website": "bolt w @brisk-docs/website run yarn dev",
        "docs": "bolt w @brisk-docs/website run dev",
        "build": "bolt w @brisk-docs/website run next build",
        "start": "next start -p 8080",
        "prettier": "prettier --write \"**/*.js\"",
        "lint:prettier": "prettier --list-different \"**/*.js\"",
        "lint:eslint": "yarn eslint .",
        "lint": "yarn lint:eslint && yarn lint:prettier"
    },
    "bolt": {
        "version": "^0.22.1",
        "workspaces": [
            "packages/*"
        ]
    },
    "dependencies": {
        "@atlaskit/avatar": "^14.1.8",
        "@atlaskit/button": "^8.2.4",
        "@atlaskit/css-reset": "^3.0.5",
        "@atlaskit/docs": "^6.0.2",
        "@atlaskit/drawer": "^2.7.1",
        "@atlaskit/dropdown-menu": "^6.1.26",
        "@atlaskit/dynamic-table": "^10.0.23",
        "@atlaskit/icon": "^16.0.1",
        "@atlaskit/logo": "^9.1.0",
        "@atlaskit/navigation-next": "^4.1.2",
        "@atlaskit/quick-search": "^5.2.5",
        "@atlaskit/section-message": "^1.0.17",
        "@atlaskit/table-tree": "^5.0.7",
        "@atlaskit/theme": "^6.0.0",
        "@atlaskit/tree": "^4.1.11",
        "@babel/preset-flow": "^7.0.0",
        "@mdx-js/loader": "^0.17.5",
