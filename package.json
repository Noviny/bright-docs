        "@mdx-js/tag": "^0.17.5",
        "@zeit/next-css": "^1.0.1",
        "@zeit/next-mdx": "^1.2.0",
        "babel-plugin-transform-flow-strip-types": "^6.22.0",
        "bolt": "^0.22.6",
        "command-line-args": "^5.0.2",
        "emotion": "^9.0.1",
        "emotion-server": "^9.2.12",
        "express": "^4.16.4",
        "extract-react-types-loader": "^0.3.0",
        "fs-extra": "^7.0.1",
        "git-url-parse": "^11.1.2",
